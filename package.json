    "@types/cors": "^2.8.3",
    "@types/express": "^4.0.39",
    "@types/faker": "^4.1.2",
    "@types/fs-extra": "^4.0.4",
    "@types/lodash": "^4.14.85",
    "@types/md5": "^2.1.32",
    "@types/mocha": "^2.2.44",
    "@types/mongoose": "^4.7.26",
    "@types/multer": "^1.3.6",
    "@types/reflect-metadata": "0.0.5",
    "@types/should": "^11.2.0",
    "@types/supertest": "^2.0.4",
    "cross-env": "^5.1.1",
    "faker": "^4.1.0",
