    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.1",
    "@nestjs/core": "^4.5.1",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "class-validator": "^0.7.3",
    "cookie-parser": "^1.4.3",
