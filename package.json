    "pretest": "npm run build",
    "test-spec": "mocha",
    "test-cov": "nyc npm run test-spec && nyc report",
    "test": "npm run test-cov"
  },
  "keywords": [
    "PandoraBox",
    "Arylo"
  ],
  "author": "AryloYeung <arylo.open@gmail.com>",
