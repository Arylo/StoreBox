{
  "name": "storebox",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc -P .",
    "build": "rm -rf dist && mkdir dist && npm run tsc && cp -r config ./dist",
